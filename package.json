  "contributors": [
    {
      "name": "Raynos"
    }
  ],
  "dependencies": {
    "aws-sdk": "^2.422.0",
    "raw-body": "^2.3.3",
    "s3rver": "2.2.8",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "coveralls": "^2.10.0",
    "istanbul": "^0.3.5",
    "opn": "^1.0.1",
    "pre-commit": "0.0.11",
    "standard": "^12.0.1",
    "tap-spec": "^2.1.1",
    "tape": "^3.4.0"
  },
  "licenses": [
    {
      "type": "MIT",
